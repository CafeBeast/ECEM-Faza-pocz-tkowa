ol
{
padding:0;
margin:0;
list-style-type:none;
font-size:18px;
height:35px;
line-height: 200%;
display: inline-block;
}
ol a 
{
	color: #ffffff;
	text-decoration: none;
	display :block;
}
ol>li
{
	float: left;
	width:150px;
	height: 40 px;
	
	
	
}
ol>li:hover
{
	background-color: black;
	
}
ol>li>ul
{
	display:none;
	list-style-type:none;
	padding: 0;
	margin: 0;
	height:40px;
}
ol>li:hover>ul
{
	background-color: black;
	display:block;
}

ol>li>ul>li
{
	background-color:#a76856;
	position: relative;
	z-index: 1000;

}
